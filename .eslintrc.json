{
    "extends": [
        "react-app",
        "airbnb-typescript",
        "prettier/@typescript-eslint",
        "plugin:prettier/recommended"
    ],
    "rules": {
        "react/prop-types": "off",
        "import/prefer-default-export": "off",
        "import/no-cycle": "off",
        "no-multi-assign": "off",
        "no-case-declarations": "off"
    },
    "settings": {
        "import/parsers": {
            "@typescript-eslint/parser": [".ts", ".tsx"]
        },
        "import/resolver": {
            "typescript": {}
        }
    },
    "env": {
      "node": true,
      "es6": true,
      "browser": true
    },
    "plugins": ["import", "jest-dom"]
}
